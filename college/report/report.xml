            <field name="report_name">college.report_student_marksheet_xlsx</field>
            <field name="report_file">college.report_student_marksheet_xlsx</field>
<!--            <field name="binding_model_id" ref="model_student_marksheet_wizard"/>-->
            <field name="binding_type">report</field>
